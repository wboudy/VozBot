# VozBot Environment Configuration
# Copy this file to .env and fill in your values

# Application
APP_ENV=development
PORT=8000

# Database
DB_URL=postgresql://vozbot:vozbot_dev@localhost:5432/vozbot

# Redis
REDIS_URL=redis://localhost:6379/0

# Speech providers (pluggable - set your provider)
STT_PROVIDER=
TTS_PROVIDER=

# LLM provider (pluggable - set your provider)
LLM_PROVIDER=

# Telephony
TELEPHONY_PROVIDER=
INBOUND_NUMBER=+1XXXXXXXXXX
TRANSFER_NUMBER=+1XXXXXXXXXX

# Logging
LOG_LEVEL=info
